{
  "name": "@kapraran/vu-ts",
  "version": "1.0.3",
  "description": "A TypeScript declaration file generator for VU-Docs. Generates TypeScript types from YAML documentation files.",
  "keywords": [
    "vu",
    "vu-docs",
    "typescript",
    "type-generator",
    "modding",
    "battlefield",
    "venice-unleashed"
  ],
  "author": "kapraran",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "bin": {
    "vu-ts": "bin/vu-ts.js"
  },
  "files": [
    "bin",
    "src",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kapraran/vu-ts.git"
  },
  "bugs": {
    "url": "https://github.com/kapraran/vu-ts/issues"
  },
  "homepage": "https://github.com/kapraran/vu-ts#readme",
  "scripts": {
    "start": "bun src/cli.ts",
    "dev": "bun --watch src/cli.ts",
    "validate": "bun scripts/validate.js",
    "infer-schema": "bun scripts/infer-schema.ts",
    "yaml-to-ts": "bun scripts/yaml-to-ts-models.ts"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "ajv": "^8.12.0",
    "jsonpath-plus": "^7.2.0",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3",
    "typescript-json-schema": "^0.55.0",
    "yaml": "^2.2.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/yargs": "^17.0.35",
    "@typescript-to-lua/language-extensions": "^1.19.0"
  }
}
